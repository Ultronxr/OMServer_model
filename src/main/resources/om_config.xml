<!--查询设备信息 http://app.newrocktech.com/omapi/chapter2/control/query/queryDeviceinfo.html-->
<?xml version="1.0" encoding="utf-8" ?>
<Control attribute="Query">
    <DeviceInfo/>
</Control>

<!--查询分机 http://app.newrocktech.com/omapi/chapter2/control/query/queryExt.html-->
<?xml version="1.0" encoding="utf-8" ?>
<Control attribute="Query">
    <ext id="001"/>
</Control>


<!-- 配置分机 -->
<?xml version="1.0" encoding="utf-8" ?>
<Control attribute="Assign">
<ext lineid="Phone 1">
    <id>001</id>
    <staffid>0000001</staffid>
    <mobile>15257845702</mobile>
    <group>1</group>
    <voicefile>busy</voicefile>
    <Call_Restriction>2</Call_Restriction>
    <Call_Pickup>no</Call_Pickup>
    <No_Disturb>no</No_Disturb>
    <Fwd_Type>1</Fwd_Type>
    <Fwd_Number>15257845702</Fwd_Number>
    <Fork>15257845702</Fork>
    <WAKEUP>00:00</WAKEUP>
    <record>on</record>
    <autoAnswer>yes </autoAnswer>
    <api>7</api>
</ext>
</Control>


